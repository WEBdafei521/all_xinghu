<view class="tui-addr-box"><form report-submit="{{true}}"><tui-list-cell vue-id="0e5f2b4b-1" hover="{{false}}" padding="0" bind:__l="__l" vue-slots="{{['default']}}"><view class="tui-line-cell"><view class="tui-title">收货人</view><input class="tui-input" placeholder-class="tui-phcolor" name="name" placeholder="请输入收货人姓名" maxlength="15" type="text" data-event-opts="{{[['input',[['__set_model',['$0','truename','$event',[]],['address']]]]]}}" value="{{address.truename}}" bindinput="__e"/></view></tui-list-cell><tui-list-cell vue-id="0e5f2b4b-2" hover="{{false}}" padding="0" bind:__l="__l" vue-slots="{{['default']}}"><view class="tui-line-cell"><view class="tui-title">手机号码</view><input class="tui-input" placeholder-class="tui-phcolor" name="mobile" placeholder="请输入收货人手机号码" maxlength="11" type="text" data-event-opts="{{[['input',[['__set_model',['$0','mobile_phone','$event',[]],['address']]]]]}}" value="{{address.mobile_phone}}" bindinput="__e"/></view></tui-list-cell><tui-list-cell vue-id="0e5f2b4b-3" arrow="{{true}}" padding="0" bind:__l="__l" vue-slots="{{['default']}}"><view data-event-opts="{{[['tap',[['showCityPicker',['$event']]]]]}}" class="tui-line-cell" bindtap="__e"><view class="tui-title"><text class="tui-title-city-text">所在城市</text></view><input class="tui-input" placeholder-class="tui-phcolor" disabled="{{true}}" name="city" placeholder="请选择城市" maxlength="50" type="text" data-event-opts="{{[['input',[['__set_model',['$0','combine_detail','$event',[]],['address']]]]]}}" value="{{address.combine_detail}}" bindinput="__e"/></view></tui-list-cell><tui-list-cell vue-id="0e5f2b4b-4" hover="{{false}}" padding="0" bind:__l="__l" vue-slots="{{['default']}}"><view class="tui-line-cell"><view class="tui-title">收货地址</view><input class="tui-input" placeholder-class="tui-phcolor" name="address" placeholder="请输入详细的收货地址" maxlength="50" type="text" data-event-opts="{{[['input',[['__set_model',['$0','address','$event',[]],['address']]]]]}}" value="{{address.address}}" bindinput="__e"/></view></tui-list-cell><tui-list-cell vue-id="0e5f2b4b-5" hover="{{false}}" padding="0" bind:__l="__l" vue-slots="{{['default']}}"><view class="tui-line-cell"><view class="tui-cell-title">地址类型</view><view class="tui-addr-label"><block wx:for="{{lists}}" wx:for-item="item" wx:for-index="index" wx:key="index"><text data-event-opts="{{[['tap',[['setAddressType',[index]]]]]}}" class="{{['tui-label-item',(index==addressIndex)?'tui-label-active':'']}}" bindtap="__e">{{item}}</text></block></view></view></tui-list-cell><tui-list-cell vue-id="0e5f2b4b-6" hover="{{false}}" padding="0" bind:__l="__l" vue-slots="{{['default']}}"><view class="tui-swipe-cell"><view>设为默认地址</view><switch class="tui-switch-small" checked="{{address.is_default==1}}" color="#D7B975" data-event-opts="{{[['change',[['defaultAddress',['$event']]]]]}}" bindchange="__e"></switch></view></tui-list-cell><block wx:if="{{!edit}}"><view data-event-opts="{{[['tap',[['saveAddress',['$event']]]]]}}" class="tui-addr-save" bindtap="__e"><button class="l-button" type="mybg" height="88rpx">保存收货地址</button></view></block><block wx:else><view data-event-opts="{{[['tap',[['updateAddress',['$event']]]]]}}" class="tui-addr-save" bindtap="__e"><button class="l-button" type="mybg" height="88rpx">更新收货地址</button></view></block><block wx:if="{{edit}}"><view data-event-opts="{{[['tap',[['delAddress',['$event']]]]]}}" class="tui-del" bindtap="__e"><button type="gray" height="88rpx">删除收货地址</button></view></block></form><u-picker vue-id="0e5f2b4b-7" mode="region" area-code="{{area_code}}" value="{{showRegion}}" data-event-opts="{{[['^cancel',[['pickerCancel']]],['^confirm',[['pickerOk']]],['^input',[['__set_model',['','showRegion','$event',[]]]]]]}}" bind:cancel="__e" bind:confirm="__e" bind:input="__e" bind:__l="__l"></u-picker><u-toast class="vue-ref" vue-id="0e5f2b4b-8" data-ref="uToast" bind:__l="__l"></u-toast></view>